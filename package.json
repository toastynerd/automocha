{
  "name": "automocha",
  "description": "autorun mocha tests on file changes",
  "version": "0.0.4",
  "keywords": [
    "mocha",
    "testing",
    "watch"
  ],
  "author": "Tyler Morgan <tyler@codefellows.com>",
  "repository" : {
    "type": "git",
    "url": "https://github.com/toastynerd/automocha"
  },
  "dependencies": {
    "glob": "^4.0.4",
    "mocha": "*"
  },
  "devDependencies": {
    "chai": "^1.9.1"
  },
  "bin": {
    "automocha": "./bin/automocha"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha test/unit/**/*-test.js"
  },
  "files": [
    "lib",
    "bin"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.github.com/toastynerd/automocha/master/LICENSE"
    }
  ]
}
